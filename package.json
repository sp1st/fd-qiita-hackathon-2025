{
  "name": "fd-qiita-hktn-2025",
  "description": "A modern full-stack template powered by Cloudflare Workers, using Hono for backend APIs, React Router for frontend routing, and shadcn/ui for beautiful, accessible components styled with Tailwind CSS",
  "private": true,
  "type": "module",
  "cloudflare": {
    "label": "React Router + Hono Fullstack App",
    "products": [
      "Workers"
    ],
    "categories": [
      "starter"
    ],
    "docs_url": "https://developers.cloudflare.com/workers",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/cd71c67a-253f-477d-022c-2f90cb4b3d00/public",
    "icon_urls": [],
    "publish": true
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.7",
    "@libsql/client": "^0.15.10",
    "@tsndr/cloudflare-worker-jwt": "^3.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/bcryptjs": "^3.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.3",
    "hono": "4.8.2",
    "isbot": "5.1.26",
    "jsonwebtoken": "^9.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "7.6.3",
    "zod": "^4.0.11"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "1.7.5",
    "@cloudflare/workers-types": "^4.20250425.0",
    "@eslint/js": "^9.32.0",
    "@react-router/dev": "7.6.3",
    "@tailwindcss/vite": "4.1.4",
    "@types/node": "24.0.4",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "tailwindcss": "4.1.1",
    "tsx": "^4.20.3",
    "typescript": "5.8.3",
    "vite": "^6.0.0",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.2.4",
    "wrangler": "4.21.x"
  },
  "scripts": {
    "build": "react-router build",
    "build:prod": "react-router build --mode production",
    "cf-typegen": "wrangler types",
    "deploy": "npm run build && wrangler deploy",
    "dev": "wrangler dev --live-reload",
    "watch-build": "nodemon --watch app --ext js,jsx,ts,tsx,css,json --exec \"npm run build\"",
    "watch-build:prod": "nodemon --watch app --ext js,jsx,ts,tsx,css,json --exec \"npm run build:prod\"",
    "dev:vite": "react-router dev",
    "dev:log": "mkdir -p logs && wrangler dev 2>&1 | tee logs/wrangler-$(date +%Y%m%d-%H%M%S).log",
    "dev:debug": "./scripts/dev-with-logging.sh",
    "preview": "npm run build:prod && vite preview",
    "typecheck": "npm run cf-typegen && react-router typegen && tsc -b",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "seed:local": "tsx workers/seed-data.ts",
    "seed:generate-sql": "tsx workers/generate-seed-sql.ts",
    "seed:prod": "npm run seed:generate-sql && wrangler d1 execute medical-consultation-db --file=./workers/seed-prod.sql --remote",
    "test": "vitest"
  }
}
